{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "APP DEBUGGER",
        "skipFiles": ["<node_internals>/**"],
        "runtimeExecutable": "npm",
        "runtimeArgs": ["run", "start-dev"],
        "outFiles": ["${workspaceFolder}/dist/**/*.js"],
        "sourceMaps": true,
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen"
      }
    ]
  }
  