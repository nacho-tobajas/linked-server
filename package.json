{
  "name": "dmcoffers-server-main",
  "version": "1.4.1",
  "type": "module",
  "description": "Proyecto para DSW-UTN",
  "main": "index.ts",
  "scripts": {
    "test-watch": "jest --watch",
    "build": "tsc -p tsconfig.build.json",
    "build-dev": "tsc -p tsconfig.json",
    "start": "node ./dist/index.js",
    "postinstall": "node -e \"if (process.env.RENDER === 'true') { require('child_process').execSync('npm install @types/express @types/cors @types/pg @types/jsonwebtoken @types/nodemailer @types/swagger-jsdoc @types/swagger-ui-express', { stdio: 'inherit' }); require('child_process').execSync('mkdir -p ./uploads/games', { stdio: 'inherit' }); }\"",
    "start-dev": "tsc-watch --noClear -p ./tsconfig.json --onSuccess \"node ./dist/index.js\"",
    "test": "jest"
  },
  "engines": {
    "node": "20.x"
  },
  "author": "Hoyos, A. ; Marcosano, M. ; Ceballos, R.; Fernandez, M.; Fernandez, J.; Tobajas Ramirez, I.",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^20.19.13",
    "argon2": "^0.40.3",
    "cloudinary": "^1.41.3",
    "connect-multiparty": "^2.2.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.6.1",
    "express": "^4.19.2",
    "express-validator": "^7.1.0",
    "inversify": "^6.0.2",
    "inversify-express-utils": "^6.4.6",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.2",
    "multer-storage-cloudinary": "^4.0.0",
    "nodemailer": "^7.0.5",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^1.4.13",
    "@types/nodemailer": "^6.4.19",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "jest": "^29.7.0",
    "supertest": "^7.1.3",
    "ts-jest": "^29.4.1",
    "tsc-watch": "^6.2.0",
    "typescript": "^4.9.5"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript"
    ]
  }
}
